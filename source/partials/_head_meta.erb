<head>
  <meta charset="utf-8">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <meta name="viewport"
        content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <!-- Use the title from a page's frontmatter if it has one -->
  <title><%= current_page.data.title || 'Middleman' %></title>
  <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css">
  <link href="http://fonts.googleapis.com/css?family=Cookie" rel="stylesheet" type="text/css">

  <link rel="apple-touch-icon" sizes="180x180" href="http://dev.animeoutreach.org/static/images/favicons/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="http://dev.animeoutreach.org/static/images/favicons/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="http://dev.animeoutreach.org/static/images/favicons/favicon-16x16.png">
  <link rel="manifest" href="http://dev.animeoutreach.org/static/images/favicons/manifest.json">
  <link rel="mask-icon" href="http://dev.animeoutreach.org/static/images/favicons/safari-pinned-tab.svg" color="#5bbad5">
  <meta name="msapplication-config" content="http://dev.animeoutreach.org/static/images/favicons/browserconfig.xml" />
  <meta name="theme-color" content="#ffffff">

  <!-- COMMON TAGS -->
  <meta charset="utf-8">
  <!-- Search Engine -->
  <meta name="description" content="Creating anime to communicate the love of Jesus anywhere and any time | アニメを作りながどこでもいつでもイエスの愛を伝えるように励ましたい">
  <meta name="image" content="http://dev.animeoutreach.org/static/images/public/og_cover_image.jpg">
  <!-- Schema.org for Google -->
  <meta itemprop="name" content="AnimeOutreach">
  <meta itemprop="description" content="Creating anime to communicate the love of Jesus anywhere and any time | アニメを作りながどこでもいつでもイエスの愛を伝えるように励ましたい">
  <meta itemprop="image" content="http://dev.animeoutreach.org/static/images/public/og_cover_image.jpg">
  <!-- Twitter -->
  <meta name="twitter:card" content="summary">
  <meta name="twitter:title" content="AnimeOutreach">
  <meta name="twitter:description" content="Creating anime to communicate the love of Jesus anywhere and any time | アニメを作りながどこでもいつでもイエスの愛を伝えるように励ましたい">
  <meta name="twitter:site" content="anime_outreach">
  <meta name="twitter:creator" content="anime_outreach">
  <meta name="twitter:image:src" content="http://dev.animeoutreach.org/static/images/public/og_cover_image.jpg">
  <!-- Open Graph general (Facebook, Pinterest & Google+) -->
  <meta name="og:title" content="AnimeOutreach">
  <meta name="og:description" content="Creating anime to communicate the love of Jesus anywhere and any time | アニメを作りながどこでもいつでもイエスの愛を伝えるように励ましたい">
  <meta name="og:image" content="http://dev.animeoutreach.org/static/images/public/og_cover_image.jpg">
  <meta name="og:url" content="http://animeoutreach.org">
  <meta name="og:site_name" content="AnimeOutreach">
  <meta name="fb:admins" content="1722414251102103">
  <meta name="og:type" content="website">

  <%= stylesheet_link_tag 'header', 'site', 'footer', 'index', 'introduction', 'visions' %>
  <%= javascript_include_tag 'site', 'classie'%>
  <script src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
</head>
